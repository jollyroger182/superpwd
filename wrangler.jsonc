{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "superpwd",
  "compatibility_date": "2026-01-01",
  "build": {
    "watch_dir": "./src",
    "command": "bun run build",
  },
  "assets": {
    "directory": "./dist-web"
  },
  "workers_dev": false,
  "preview_urls": false,
  "routes": [
    {
      "custom_domain": true,
      "pattern": "superpwd.jollyy.dev"
    }
  ]
}