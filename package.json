{
  "name": "superpwd",
  "module": "dist/index.js",
  "type": "module",
  "version": "1.3.1",
  "bin": {
    "superpwd": "./cli.ts"
  },
  "scripts": {
    "build": "bun run build-web && bun run build-lib",
