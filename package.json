    "wrangler": "^4.59.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "argon2id": "^1.0.1",
    "bootstrap": "^5.3.8",
    "prompt": "^1.3.0",
    "yargs": "^18.0.0"
  }
}
